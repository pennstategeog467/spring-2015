#PSU_Centroids_Labels [zoom=15] [Shape_Area >=50000] [Shape_Area<75000] {//add large area names
  text-name: [Shortened1];
  text-face-name: 'Kievit Offc Pro Medium';
  text-size: 10;
  text-clip: [Cross];
  text-wrap-width: 10;
  text-character-spacing: -0.5;
  text-line-spacing: -2;
  text-fill: white;
  text-halo-opacity: 0.7; 
  text-halo-fill: #222c4a;
  text-halo-radius: 2;
  text-avoid-edges: true;
  text-margin: 15;
}

#Major_Campus_Areas [zoom=15] {
  text-name: [Full_Funct];
  text-face-name: 'Kievit SC Offc Pro Bold';
  text-size: 11;
  text-clip: [Cross];
  text-wrap-width: 10;
  text-character-spacing: -0.5;
  text-line-spacing: -2;
  text-fill: white;
  text-halo-opacity: 0.7; 
  text-halo-fill: #222c4a;
  text-halo-radius: 1.5;
  text-avoid-edges: true;
  text-margin: 15;  
}

#road_label [zoom=15] {
  text-name: [name];
  text-face-name: 'Tisa SC Offc Pro Medium';
  text-size: 11;
  text-fill: #fff;
  text-halo-opacity: 0.7; 
  text-halo-fill: #626363;
  text-halo-radius: 3;
  text-placement: line;
  text-repeat-distance: 500;
  text-min-padding: 25; 
  text-min-path-length: 50;
  text-margin: 4;
  text-avoid-edges: true;
}

